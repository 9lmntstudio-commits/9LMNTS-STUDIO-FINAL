{
  "version": 2,
  "name": "9LMNTS-ULTIMATE",
  "builds": [
    {
      "src": "apps/website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/event-os",
      "dest": "/apps/website"
    },
    {
      "src": "/loa-assistant",
      "dest": "/apps/website"
    },
    {
      "src": "/loa-game",
      "dest": "/apps/website"
    },
    {
      "src": "/(.*)",
      "dest": "/apps/website/$1"
    }
  ]
}
